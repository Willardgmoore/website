function IsDate(mydate)
{
  var mycase = 1;
  var checkOK = "0123456789/";
  var checkStr = mydate;
  var allValid = true;
  for (i = 0;  i < checkStr.length;  i++)
  {
    ch = checkStr.charAt(i);
    for (j = 0;  j < checkOK.length;  j++)
      if (ch == checkOK.charAt(j))
        break;
    if (j == checkOK.length)
    {
		alert("Sorry, Date must be of the form mm/dd/yy");
		return (false);
    }
	if (mycase == 1) 
	{
		if (IsNumeric(ch) == true)
			mycase = 2;
	} 
	if (mycase == 2) 
	{
		if (ch == "/")
			mycase = 3;
	}
	if (mycase == 3) 
	{
		if (IsNumeric(ch) == true)
			mycase = 4;
	}
	if (mycase == 4) 
	{
		if (ch == "/")
			mycase = 5;
	}
	if (mycase == 5) 
	{
		if (IsNumeric(ch) == true)
			mycase = 6;
	}
  }
  if (mycase == 6)
	return(true);
  if (mydate == "")
    return(true);
  alert("Sorry, Date must be of the form mm/dd/yy");
