			function recalculateAllColorOverlayImageSize(){
				var somePadding = 100;
				var maxWidth = window.innerWidth - somePadding;
				var maxHeight = window.innerHeight - somePadding;
				
				
				var tagNum = 0;
				$("div[class^=color_overlay]").each(function() {
					tagNum++;
					var imgHeight = $("#color" + tagNum + " img").attr('actualHeight');
					var imgWidth  = $("#color" + tagNum + " img").attr('actualWidth');

					var newImgHeight = imgHeight;
					var	newImgWidth = imgWidth;
					// recalculate height and width
					if (imgHeight > maxHeight || imgWidth > maxWidth){
						var w, h, f;
						w = imgWidth;
						h = imgHeight;
						if(h > maxHeight){
							f = maxHeight / h;
							h = maxHeight;
							w = w * f;
						}
						if(w > maxWidth){
							f = maxWidth / w;
							w = maxWidth;
							h = h * f;
						}
						newImgWidth = w;
						newImgHeight = h;
					}
					$("#color" + tagNum + " img").attr('height',newImgHeight);
					$("#color" + tagNum + " img").attr('width', newImgWidth);
				});
			};
			
